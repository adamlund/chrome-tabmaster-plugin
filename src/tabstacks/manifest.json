{
	"manifest_version" : 2,

	"name" : "Tab Stacks: A menu listing your open tabs",
	"description" : "View and search through open tabs and search history to find what you're looking for.",
	"version" : "1.0.4",
	"icons": {
		"16": "img/logo32x.png",
		"48": "img/logo64x.png",
		"64": "img/logo64x.png",
		"128": "img/logo128x.png"
	},
	"browser_action" : {
		"default_icon": {
			"19": "img/logo32x.png",
			"38": "img/logo32x.png"
        },
		"default_popup" : "tabstacks.html",
		"default_title" : "Tab Stacks"
	},
	"commands" : {
		"_execute_browser_action": {
			"suggested_key" : {
				"windows" : "Ctrl+Shift+T",
				"mac" :	"Ctrl+Shift+T"
			}
		}
	},
	"permissions" : [
		"activeTab",
		"tabs",
		"history",
		"chrome://favicon/*",
		"<all_urls>"
	]

}